@using SchoolPr.Models;
@model List<Teacher>;

<table class="table table-dark table-striped text-center"
       style="max-width: 600px">
    <thead>
        <tr>
            <th>Id</th>
            <th>FirstName</th>
            <th>LastName</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in @Model)
        {
            <tr>
                <td>@teacher.Id</td>
                <td>@teacher.FirstName</td>
                <td>@teacher.LastName</td>
            </tr>
        }
    </tbody>
</table>